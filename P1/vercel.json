{
    "builds": [
        {
            "src": "app1/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
            {
                "src": "/(.*)",
                "dest": "app1/wsgi.py"
            }
        ]
}